<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marolix.androidonlinequiz">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_welcome"
            android:theme="@style/AppTheme1" />
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainCategory"
            android:label="@string/title_activity_main_category"
            android:theme="@style/AppTheme" />
        <activity android:name=".Signup" />
        <activity android:name=".TermOfUsage" />
        <activity android:name=".Algebra" />
        <activity android:name=".Subcategories" />
        <activity android:name=".Question" />
        <activity android:name=".Congratulation" />
        <activity android:name=".ResultAnalysis"></activity>
    </application>

</manifest>